# Default distro etc
dist=debian
rel=bullseye
arch=amd64
# Container location
contlocation=/home/$USER/.local/share/lxc
# If a bridge is available assign it here, otherwise leave as bridge=
bridge=
# The prefix for ethernet interfaces usually enp or eth
ethernetprefix=enp
# Default VNI
vni=lxcbr0
# Git
gitemail=
gitname=
# Locale settings
localelang=en_GB.UTF-8
localekeymap=gb
localex11keymap=gb

